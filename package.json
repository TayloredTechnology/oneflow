  "homepage": "https://github.com/TayloredTechnology/oneflow#readme",
  "version": "0.0.1",
  "bin": {
    "of": "./cli.js",
    "oneflow": "./cli.js"
  },
  "author": {
    "email": "TayloredTechnology@protonmail.ch",
    "name": "Taylored Technology"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TayloredTechnology/oneflow.git"
  },
  "bugs": {
    "url": "https://github.com/TayloredTechnology/oneflow/issues"
  },
  "dependencies": {
    "co": "^4.6.0",
    "conventional-recommended-bump": "^2.0.6",
    "debug": "^3.1.0",
    "execa": "^0.10.0",
    "pino": "^4.14.0",
    "pino-debug": "^1.1.1",
    "read-pkg": "^3.0.0",
    "simple-git": "^1.92.0",
    "yargs": "^11.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^6.1.3",
    "@commitlint/config-conventional": "^6.1.3",
    "@commitlint/prompt-cli": "^6.1.3",
    "conventional-changelog-cli": "^1.3.16",
    "husky": "^0.14.3",
    "prettier": "^1.11.1",
    "release-it": "^7.2.1",
    "xo": "^0.20.3"
  },
  "scripts": {
    "commit": "commit",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "release": "release-it"
  },
  "license": "SEE LICENSE IN LICENSE",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "xo": {
    "globals": [],
    "prettier": true,
    "rules": {
      "generator-star-spacing": "off",
      "import/no-extraneous-dependencies": "off",
      "import/no-unassigned-import": "off",
      "import/no-unresolved": "off",
      "import/order": "off",
      "new-cap": [
        {
          "newIsCap": true,
          "capIsNew": true,
          "capIsNewExceptions": []
        },
        "error"
      ],
      "operator-linebreak": "off"
    },
    "semicolon": false
  }
}
